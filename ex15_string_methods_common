package helloWorld;


public class Main {

	public static void main(String[] args) {
		
		String name = "ada";
		String name2 = "";
		
		boolean result = name.equals("ada");
		boolean result1 = name.equalsIgnoreCase("aDa");
		int result2 = name.length();
		char result3 = name.charAt(0);
		int result4 = name.indexOf("d");
		boolean result5 = name2.isEmpty();
		String result6 = name.toUpperCase();
		String result7 = name.trim();
		String result8 = name.replace("d", "T");
		
		System.out.println(result);
		System.out.println(result1);
		System.out.println(result2);
		System.out.println(result3);
		System.out.println(result4);
		System.out.println(result5);
		System.out.println(result6);
		System.out.println(result8);
		
	}
} 
